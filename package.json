{
  "name": "thesoreon.com",
  "description": "Pavel Susicky's personal website/blog",
  "repository": "git@github.com:Thesoreon/thesoreon.com.git",
  "author": "Pavel Susicky <pavels.sus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop --open",
    "start:no": "gatsby develop",
    "start:network": "gatsby develop --open --host 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve --open",
    "serve:network": "gatsby serve --open --host 0.0.0.0",
    "gatsby:info": "gatsby info --clipboard",
    "gatsby:clean": "gatsby clean",
    "test": "jest --rootDir --config test/jest.config.js",
    "test:watch": "jest --watch --rootDir --config test/jest.config.js",
    "test:lint": "eslint --ignore-path .eslintignore --ext ts,tsx,js,jsx .",
    "test:tsc": "tsc",
    "test:e2e": "start-server-and-test start:no http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start:no http://localhost:8000 cy:run",
    "typegen": "graphql-codegen --config codegen.yml && eslint --fix --no-ignore src/graphqlTypes.ts",
    "check:prettier": "prettier --check 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext ts,tsx,js,jsx .",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.17.4",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-emotion": "^4.1.12",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-manifest": "^2.2.26",
    "gatsby-plugin-mdx": "^1.0.53",
    "gatsby-plugin-nprogress": "^2.1.12",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.36",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-remark-images": "^3.1.29",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.2",
    "pixi.js": "^5.2.0",
    "polished": "^3.4.2",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-loadable": "^5.5.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/typescript": "1.8.2",
    "@graphql-codegen/typescript-operations": "1.8.2",
    "@testing-library/jest-dom": "^4.2.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.20",
    "@types/react-helmet": "^5.0.13",
    "@types/react-loadable": "^5.5.3",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-plugin-root-import": "^6.4.1",
    "babel-preset-gatsby": "^0.2.20",
    "cypress": "^3.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.17",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2"
  }
}
